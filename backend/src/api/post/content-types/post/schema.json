{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "text"
    },
    "original_url": {
      "type": "string",
      "unique": true
    },
    "published": {
      "type": "datetime"
    },
    "clickCount": {
      "type": "biginteger",
      "default": "0"
    },
    "source": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::source.source",
      "inversedBy": "posts"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "posts"
    },
    "viewCount": {
      "type": "biginteger",
      "default": "0"
    },
    "view_log": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::view-log.view-log",
      "mappedBy": "post"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
