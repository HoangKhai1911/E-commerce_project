{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "original_url": {
      "type": "string"
    },
    "published": {
      "type": "datetime"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "posts"
    },
    "sources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::source.source",
      "mappedBy": "post"
    }
  }
}
