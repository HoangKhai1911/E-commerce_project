{
  "kind": "collectionType",
  "collectionName": "sources",
  "info": {
    "singularName": "source",
    "pluralName": "sources",
    "displayName": "Source"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "URL": {
      "type": "string",
      "required": true
    },
    "homepage_url": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "sources"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "source"
    },
    "lastCrawledAt": {
      "type": "datetime",
      "default": null
    },
    "errorCount": {
      "type": "integer",
      "default": 0
    },
    "lastErrorAt": {
      "type": "datetime",
      "default": null
    }
  }
}
